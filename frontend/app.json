{
  "expo": {
    "name": "MOOD",
    "slug": "mood-fitness-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "moodapp",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.official.moodapp",
      "buildNumber": "23",
      "supportsTablet": true,
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": false,
        "UIStatusBarStyle": "UIStatusBarStyleLightContent",
        "NSCameraUsageDescription": "Take photos and videos of your workouts",
        "NSPhotoLibraryUsageDescription": "Share your workout progress photos",
        "NSPhotoLibraryAddUsageDescription": "Save workout photos to your library",
        "NSMicrophoneUsageDescription": "Record audio for your workout videos",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.official.moodapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "backgroundColor": "#000000",
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 150,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "./plugins/withCxx17Fix",
      "expo-font",
      "expo-web-browser"
    ],
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_BACKEND_URL": "https://bug-busters-13.emergent.host",
      "BACKEND_URL": "https://bug-busters-13.emergent.host",
      "router": {},
      "eas": {
        "projectId": "ef2c8520-832c-4806-beb0-80b9db1a6214"
      }
    }
  }
}
